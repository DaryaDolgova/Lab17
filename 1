#include <iostream>

using namespace std; 

int main()

{
    int i,n,l,j,t,r;
    
    cout<<"N= "; cin>>n;
    int *A=new int[n];
    int *B=new int[l]; 
    int *C=new int[l];
    l=1;
    for (i=0; i<n; i++){cin>>A[i];}
    for (i=1; i<n; i++){
        if (A[i]!=A[i-1]){
            l++;
        } 
    }
    C[0]=A[0];j=0;
    for (i=1; i<n;i++){
        if (A[i]!=A[i-1]) {
            j++;
            C[j]=A[i];
        } 
    }
    r=0; t=1;
    for (i=0; i<n-1;i++){
        if (A[i]!=A[i+1]) {
            B[r]=t;
            t=1;
            r++;
        }
        else {
            t++;
            B[r]=t;
        }
    }
    cout<<"ะก= ";
    for (j=0; j<l; j++){cout<<C[j]<<" "<<ends;}
    cout<<"B= ";
    for (r=0; r<l; r++){cout<<B[r]<<" "<<ends;}

return 0;
} 
